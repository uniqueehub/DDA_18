#include<iostream>
using namespace std;
int count(int a[],int n,int key){
    int i,count=0;
    for(int i=0;i<=n;i++){
        if(a[i]==key){
            count++;
        }
    }
    return count;
}
int count(char b[],int m,char cha){
    int i,count=0;
    for(int i=0;i<=m;i++){
        if(b[i]==cha){
            count++;
        }
    }
    return count;
}
